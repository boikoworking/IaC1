{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    "storagePrefix": {
      "type": "string"
    },
    "storageSKU": {
      "type": "string"
    },
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]"
    },
    "resourceTags": {
      "type": "object"
    },
    "storagekind": {
          "type": "string"
      },
      "vnetName": {
      "type": "string"
    },
    "vnetAddressPrefix": 
    {
      "type": "string"
    },
    "subnet1Prefix": {
      "type": "string"
    },
    "subnet1Name": {
      "type": "string"
    },
    "subnet2Prefix": {
      "type": "string"
    },
    "subnet2Name": {
      "type": "string"
    },
    "containerSasToken": {
      "type": "string",
      "defaultValue": ""
    }
  },
  "variables": {
    "uniqueStorageName": "[concat(parameters('storagePrefix'), uniqueString(resourceGroup().id))]",
    "uniqueVnetname": "[concat(resourceGroup().name,parameters('vnetName'),uniqueString(resourceGroup().id))]",
    "st_acc_link": "[concat(uri(deployment().properties.templateLink.uri,'storage_account.json'),parameters('containerSasToken'))]",
    "parameters_link": "[concat(uri(deployment().properties.templateLink.uri,'parametrs.json'),parameters('containerSasToken'))]",
    "vn_link": "[concat(uri(deployment().properties.templateLink.uri,'virtual_network.json'),parameters('containerSasToken'))]"
  },
  

    "resources": [
          {
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "name": "linkedTemplate",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri":"[variables('st_acc_link')]",
            "contentVersion":"1.0.0.0"
          },
          "parametersLink": {
               "uri": "[variables('parameters_link')]" 
              }
            }
          },
           {
        "type": "Microsoft.Resources/deployments",
        "apiVersion": "2021-04-01",
        "name": "linkedTemplatett",
        "properties": {
          "mode": "Incremental",
          "templateLink": {
            "uri":"[variables('vn_link')]",
            "contentVersion":"1.0.0.0"
              },
            "parametersLink": {
               "uri": "[variables('parameters_link')]" 
              }
            }
          }
    ],
    "outputs": {
    }
  }
